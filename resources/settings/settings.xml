<resources>
	<properties>
		<property id="PROP_USERNAME" type="string"></property>
		<property id="PROP_PASSWORD" type="string"></property>
		<property id="PROP_PROTOCOL" type="number">1</property>
		<property id="PROP_ADRESS" type="string"></property>
		<property id="PROP_PATH" type="string"></property>
		<property id="PROP_PORT" type="number">443</property>
		<property id="PROP_INTERFACE" type="number">1</property>
	</properties>
	<settings>
		<setting propertyKey="@Properties.PROP_USERNAME" title="@Strings.title_username">
			<settingConfig type="AlphaNumeric"></settingConfig>
		</setting>
		<setting propertyKey="@Properties.PROP_PASSWORD" title="@Strings.title_password">
			<settingConfig type="password"></settingConfig>
		</setting>
		<setting propertyKey="@Properties.PROP_PROTOCOL" title="@Strings.title_protocol">
			<settingConfig type="list" required="true">
				<listEntry value="1">@Strings.HTTPS</listEntry>
			</settingConfig>
		</setting>
		<setting propertyKey="@Properties.PROP_ADRESS" title="@Strings.title_adress">
			<settingConfig type="AlphaNumeric" required="true"></settingConfig>
		</setting>
		<setting propertyKey="@Properties.PROP_PATH" title="@Strings.title_path">
			<settingConfig type="AlphaNumeric" required="false"></settingConfig>
		</setting>
		<setting propertyKey="@Properties.PROP_PORT" title="@Strings.title_port">
			<settingConfig type="numeric" required="true"></settingConfig>
		</setting>
		<setting propertyKey="@Properties.PROP_INTERFACE" title="@Strings.title_interface">
			<settingConfig type="list" required="true">
				<listEntry value="1">@Strings.LEGACY</listEntry>
				<listEntry value="2">@Strings.EXPERIMENTAL</listEntry>
				<listEntry value="3">@Strings.AUTO</listEntry>
			</settingConfig>
		</setting>
	</settings>
</resources>